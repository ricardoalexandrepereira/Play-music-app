<!DOCTYPE html>
<html lang="pt">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Music App</title>
  </head>
  <body>
    <h1>hello ejs</h1>

    <a href="/admin">administração</a>

    <section>

      <% playlist.forEach((music)=>{ %>

      <h2><%= music.name %></h2>
      <p><%= music.author %></p>
      <img src="<%= music.linkImage %>" alt="link da música" />
      <audio controls src="<%= music.linkMusic %>"></audio>

      <% }) %>
    </section>

  </body>
</html>
